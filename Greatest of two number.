def maximum(num1,num2):
#must follow indentation.
    if num1>num2:
        print(num1,'is greatest.')
    elif num1==num2:
        print('Both are equal.')    
    else:
        print(num2,'is greatest.')

num1 = float(input('Enter first number.'))
num2 = float(input('Enter second number.'))            
maximum(num1,num2)
